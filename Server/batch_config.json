{
  "scripts": ["Source/Server.py"],
  "config": {
    "onefile": true,
    "noconsole": false,
    "clean_build": true,
    "name": "JurisDataServer",
    "output_dir": "bin",
    "build_dir": "../build",

    "hidden_imports": [
      "playwright",
      "playwright.async_api",
      "playwright.sync_api",
      "lxml",
      "lxml.etree",
      "lxml._elementpath",
      "lxml.html",
      "lxml.builder",
      "parsel"
    ],

    "collect_submodules": [
      "playwright",
      "lxml",
      "parsel"
    ],

    "collect_metadata": [
      "playwright",
      "lxml",
      "parsel"
    ],

    "exclude_modules": [
      "twisted",
      "scrapy",
      "zope.interface",
      "queuelib",
      "w3lib",
      "protego",
      "OpenSSL",
      "cryptography",
      "pyasn1"
    ],

    "add_data": [
      "Source/Config.json;.",
      "Source/ParserEngine;ParserEngine",
      "Source/Worker;PlaywrightWorker"
    ]
  }
}
